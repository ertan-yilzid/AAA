<html>
<head>
    <meta>
</head>
<body>
    <h1>Hostname: {{NAME}}</h1>
    <h1>OS: {{OS_FULL}}</h1>
    <h1>Boot Time: {{BOOT_TIME}}</h1>
    <h1>Uptime: {{UPTIME}}</h1>
    <h1>Current Time: {{CURRENT_TIMESTAMP}}</h1>
    <h1>Users: {{USER_COUNT}} ({{USER_NAMES}})</h1>
    
    <h1>CPU:</h1>
    <h2>Cores: {{CORES}} (Physical: {{CORES_PHYSICAL}}, Logical: {{CORES_LOGICAL}})</h2>
    <h2>Clock Rate:<br>
        CURRENT: {{CURR_CL}} MHz<br>
        MIN: {{MIN_CL}} MHz<br>
        MAX: {{MAX_CL}} MHz
    </h2>
    <h2>CPU Usage: {{CPU}}%</h2>
    
    <h1>RAM:</h1>
    <h2>TOTAL: {{TOTAL_RAM_GB}} GB</h2>
    <h2>IN USE: {{IN_USE_GB}} GB</h2>
    <h2>AVAILABLE: {{AVAILABLE_RAM_GB}} GB</h2>
    <h2>PERCENT: {{PERCENT_RAM}}%</h2>
    
    <h1>DISK:</h1>
    <h2>TOTAL: {{TOTAL_DISK_GB}} GB</h2>
    <h2>USED: {{USED_DISK_GB}} GB</h2>
    <h2>FREE: {{FREE_DISK_GB}} GB</h2>
    <h2>PERCENT: {{PERCENT_DISK}}%</h2>
    
    <h1>IP: {{MAIN_IP}}</h1>
    
    <h1>PROCESSES: {{PROCESS_COUNT}} total</h1>
    
    <h1>TOP 3 CPU:</h1>
    <table border="1">
        <tr>
            <th>PID</th>
            <th>NAME</th>
            <th>MEMORY %</th>
            <th>CPU %</th>
        </tr>
        <tr>
            <td>{{CPU_PID1}}</td>
            <td>{{CPU_NAME1}}</td>
            <td>{{CPU_MEMORY1}}</td>
            <td>{{CPU_CPU1}}</td>
        </tr>
        <tr>
            <td>{{CPU_PID2}}</td>
            <td>{{CPU_NAME2}}</td>
            <td>{{CPU_MEMORY2}}</td>
            <td>{{CPU_CPU2}}</td>
        </tr>
        <tr>
            <td>{{CPU_PID3}}</td>
            <td>{{CPU_NAME3}}</td>
            <td>{{CPU_MEMORY3}}</td>
            <td>{{CPU_CPU3}}</td>
        </tr>
    </table>
    
    <h1>TOP 3 RAM:</h1>
    <table border="1">
        <tr>
            <th>PID</th>
            <th>NAME</th>
            <th>MEMORY %</th>
            <th>CPU %</th>
        </tr>
        <tr>
            <td>{{RAM_PID1}}</td>
            <td>{{RAM_NAME1}}</td>
            <td>{{RAM_MEMORY1}}</td>
            <td>{{RAM_CPU1}}</td>
        </tr>
        <tr>
            <td>{{RAM_PID2}}</td>
            <td>{{RAM_NAME2}}</td>
            <td>{{RAM_MEMORY2}}</td>
            <td>{{RAM_CPU2}}</td>
        </tr>
        <tr>
            <td>{{RAM_PID3}}</td>
            <td>{{RAM_NAME3}}</td>
            <td>{{RAM_MEMORY3}}</td>
            <td>{{RAM_CPU3}}</td>
        </tr>
    </table>
    
    <h1>TOP 3 OVERALL:</h1>
    <table border="1">
        <tr>
            <th>PID</th>
            <th>NAME</th>
            <th>MEMORY %</th>
            <th>CPU %</th>
        </tr>
        <tr>
            <td>{{TOP_PID1}}</td>
            <td>{{TOP_NAME1}}</td>
            <td>{{TOP_MEMORY1}}</td>
            <td>{{TOP_CPU1}}</td>
        </tr>
        <tr>
            <td>{{TOP_PID2}}</td>
            <td>{{TOP_NAME2}}</td>
            <td>{{TOP_MEMORY2}}</td>
            <td>{{TOP_CPU2}}</td>
        </tr>
        <tr>
            <td>{{TOP_PID3}}</td>
            <td>{{TOP_NAME3}}</td>
            <td>{{TOP_MEMORY3}}</td>
            <td>{{TOP_CPU3}}</td>
        </tr>
    </table>
    
    <h1>FILE ANALYSIS</h1>
    <h2>Directory: {{PATH}}</h2>
    <h2>Directory Size: {{DIRECTORY_SIZE_MB}} MB</h2>
    <h2>Total Files: {{TOTAL_FILES_COUNT}}</h2>
    
    <h3>File Types:</h3>
    <table border="1">
        <tr>
            <th>Extension</th>
            <th>Count</th>
            <th>Percentage</th>
        </tr>
        <tr>
            <td>.txt</td>
            <td>{{TXT_COUNT}}</td>
            <td>{{TXT_PERCENT}}%</td>
        </tr>
        <tr>
            <td>.py</td>
            <td>{{PY_COUNT}}</td>
            <td>{{PY_PERCENT}}%</td>
        </tr>
        <tr>
            <td>.pdf</td>
            <td>{{PDF_COUNT}}</td>
            <td>{{PDF_PERCENT}}%</td>
        </tr>
        <tr>
            <td>.jpg</td>
            <td>{{JPG_COUNT}}</td>
            <td>{{JPG_PERCENT}}%</td>
        </tr>
    </table>
    
    <h2>Monitored Files ({{SET_FILES_COUNT}} total): {{FILES_WITH_SET_EXT}}</h2>
    <h2>All Files: {{FILES_NAME}}</h2>
</body>
</html>